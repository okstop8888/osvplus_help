  "gitbook": {
    "properties": {
      "hostname": {
        "type": "string",
        "required": true,
        "description": "Hostname for the book"
      },
      "title": {
        "type": "string",
        "required": true,
        "description": "Feed Title"
      },
      "filename": {
        "type": "string",
        "required": true,
        "description": "Filename to the feed."
      },
      "feed_type": {
        "type": "string",
        "required": true,
        "description": "Feed type: RSS 2.0 or Atom 1.0."
      },
      "author": {
        "type": "string",
        "required": true,
        "description": "Who manages content in this feed."
      },
      "description": {
        "type": "string",
        "description": "A short description of the feed."
      },
      "id": {
        "type": "string",
        "description": "ID for the feed"
      },
      "link": {
        "type": "string",
        "description": "Url to the feed."
      },
      "image_url": {
        "type": "string",
        "description": "Small image for feed readers to use."
      },
      "copyright": {
        "type": "string",
        "description": "Copyright information for this feed."
      },
      "generator": {
        "type": "string",
        "description": "Feed generator."
      },
      "categories": {
        "type": "array",
        "description": "One or more categories this feed belongs to."
      }
    }
  },
  "keywords": [
    "gitbook",
    "plugin",
    "feed",
    "rss",
    "atom"
