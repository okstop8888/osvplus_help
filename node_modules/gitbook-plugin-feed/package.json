  "description": "Feed(RSS/Atom) for GitBook",
  "main": "index.js",
  "version": "0.0.5",
  "engines": {
    "gitbook": "*"
  },
  "homepage": "https://github.com/billryan/gitbook-plugin-feed",
  "repository": {
    "type": "git",
